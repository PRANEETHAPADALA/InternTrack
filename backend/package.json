      "express-rate-limit": "^7.1.5",
      "googleapis": "^129.0.0",
      "helmet": "^7.1.0",
      "jsonwebtoken": "^9.0.2",
      "mongoose": "^8.0.3",
      "morgan": "^1.10.0",
      "multer": "^1.4.5-lts.1",
      "pdf-parse": "^1.1.1",
      "tesseract.js": "^5.0.3"
